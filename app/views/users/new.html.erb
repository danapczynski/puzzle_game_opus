<%= form_for @user do |t| %>
  <%= t.label :name %>
  <%= t.text_field :name %>
  <%= t.label :email %>
  <%= t.text_field :email %>
  <%= t.label :password %>
  <%= t.password_field :password %>
  <%= t.label :password_confirmation %>
  <%= t.password_field :password_confirmation %>
  <%= t.submit %>
<% end %>